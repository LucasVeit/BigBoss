<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bigboss">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/foto_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BigBoss">
        <activity
            android:name=".BoxExercicioAndamento"
            android:exported="true" />
        <activity
            android:name=".TelaInicioNOVA"
            android:exported="true" />
        <activity
            android:name=".telaPerfil"

            android:screenOrientation="portrait" />
        <activity
            android:name=".telaInfoExercicio"

            android:screenOrientation="portrait" />
        <activity
            android:name=".telaAdicionarExercicio"

            android:screenOrientation="portrait"/>
        <activity
            android:name=".HomeActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".telaEditarExercicio"

            android:screenOrientation="portrait" />
        <activity
            android:name=".TelaPerguntasFrequente"

            android:screenOrientation="portrait" />
        <activity
            android:name=".telaConfiguracao"
            android:parentActivityName=".HomeActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".telaInicio"

            android:screenOrientation="portrait" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>